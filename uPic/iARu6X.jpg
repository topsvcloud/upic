<div class="col-md-8 mx-auto">
<div class="nc-rating-container">
<div class="nc-rating-title">Rate this tool:</div>
<div class="nc-rating">
<span class="nc-stars" onclick="ncRate(1)">&#9733;</span>
<span class="nc-stars" onclick="ncRate(2)">&#9733;</span>
<span class="nc-stars" onclick="ncRate(3)">&#9733;</span>
<span class="nc-stars" onclick="ncRate(4)">&#9733;</span>
<span class="nc-stars" onclick="ncRate(5)">&#9733;</span>
<div class="nc-rating-num">
<span id="ncAverageRating">5</span>&#65288;<span id="ncRatingCount">1</span> votes&#65289;
</div>
</div>
</div>
</div>
</div>
</div>
</section><style>
    .nc-rating-container {
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .nc-rating-title {
        margin-right: 20px;
    }

    .nc-rating {
        display: flex;
        align-items: center;
    }

    .nc-stars {
        font-size: 24px;
        cursor: pointer;
        color: #ccc;
    }

    .nc-stars:hover,
    .nc-stars:hover ~ .nc-stars {
        color: orange;
    }

    .nc-rating-num {
        margin-left: 20px;
    }
</style><script>
    let ratingCount = 0;

    function ncRate(stars) {
        ratingCount++;
        document.getElementById('ncRatingCount').textContent = ratingCount;
        const starsElements = document.querySelectorAll('.nc-stars');
        starsElements.forEach((star, index) => {
            if (index < stars) {
                star.style.color = 'orange';
            } else {
                star.style.color = '#ccc';
            }
        });
    }

    document.addEventListener("DOMContentLoaded", function () {
        const jsonLD = document.querySelector('script[type="application/ld+json"]');
        if (jsonLD) {
            const ldData = JSON.parse(jsonLD.textContent);
            const rating = parseFloat(ldData.aggregateRating?.ratingValue) || 5;
            ratingCount = parseInt(ldData.aggregateRating?.ratingCount) || 1;

            updateRating(rating, ratingCount);
        } else {
            updateRating(5, 1);
        }
    });

    function updateRating(rating, ratingCount) {
        document.getElementById('ncAverageRating').textContent = rating.toFixed(1);
        document.getElementById('ncRatingCount').textContent = ratingCount;

        const stars = document.querySelectorAll('.nc-stars');
        stars.forEach((star, index) => {
            if (index < rating) {
                star.style.color = 'orange';
            } else {
                star.style.color = '#ccc';
            }
