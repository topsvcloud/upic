<script>    $(document).ready(function() {
        //获取cookie的值
        var username = getCookie('username');
        var password = getCookie('password');
        //alert(username + '+' + password);

        if (username != null && username != '' && password != null && password != '') {//选中保存秘密的复选框
            //将获取的值填充入输入框中
            $('#login-form-login').val(username);
            $('#login-form-password').val(password);
            $("#login-form-rememberme").attr('checked', true);
        }
    });

    $('#submit-form').on('click', function () {
        if ($("#login-form-rememberme").prop("checked") == true) {  //客户端记住用户名和密码
            setCookie("username", $('#login-form-login').val(), 14);
            setCookie("password", $('#login-form-password').val(), 14);
        } else {
            setCookie("username", "", -1);
            setCookie("password", "", -1);
        }
        $('#login-form').submit();
    })

    function setCookie(c_name,value,expiredays)
    {
        var exdate=new Date()
        exdate.setDate(exdate.getDate()+expiredays)
        document.cookie=c_name+ "=" +escape(value)+
            ((expiredays==null) ? "" : ";expires="+exdate.toGMTString())
    }

    function getCookie(c_name)
    {
        if (document.cookie.length>0)
        {
            c_start=document.cookie.indexOf(c_name + "=")
            if (c_start!=-1)
            {
                c_start=c_start + c_name.length+1
                c_end=document.cookie.indexOf(";",c_start)
                if (c_end==-1) c_end=document.cookie.length
                return unescape(document.cookie.substring(c_start,c_end))
            }
        }
        return ""
    }

    $(document).keyup(function(event){
        if(event.keyCode ==13){
            $('#submit-form').click();
        }
    });
</script>
